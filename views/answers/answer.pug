extends ../layout.pug
include ../utils.pug
block content
    h1 Your Answers
        #answer-container
                each answer in answers
                    div.single-answer(id=`answer-${answer.id}`)
                        .answer-content
                            .answer-title: p= answer.title
                            .answer-meme: img(src=`${answer.memeUrl}`)
